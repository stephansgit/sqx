.PHONY: all clean
.DELETE_ON_ERROR:


all: /var/www/html/sqx-test.servebeer.com/VolBreakout-Intraday.html

clean: 
	rm -f VolBreakout-Intraday.html data/Intraday-Data.RData
	
# ---------------------------------------------------------------------


data/Intraday-Data.RData: 05_load_intraday.R data/EOD-Data.RData
	Rscript $< 


VolBreakout-Intraday.html: VolBreakout-Intraday.Rmd data/Intraday-Data.RData
	Rscript -e "rmarkdown::render('$<')"


/var/www/html/sqx-test.servebeer.com/VolBreakout-Intraday.html: VolBreakout-Intraday.html
	mv $< $@

